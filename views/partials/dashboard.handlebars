<div class="galeria-productos py-3 row">
    {{#each productos}}
    <div class="contenedor-producto col-12 col-sm-6 col-md-4 " data-nombre="{{this}}">
        <p class="fs-3">{{this}}</p>
        <img src="/assets/img/{{this}}.png" alt="banana" class="d-block mx-auto">
    </div>
    {{/each}}
</div>

<style>
    .contenedor-producto img{
        width: 60%
    }
</style>